var popupModule2={data:{popupView:String,isMulti:!1},popupShow:function(t,i=!1,s){this.data.popupView=t,$(t).toggleClass("view"),this.popCenter(),this.data.isMulti=i,i?this.setMulti():this.setPopup(s)},setMulti:function(){$(".multiClose").click(function(){popupModule.multiClose()})},multiClose:function(){this.data.isMulti=!1,$(".popup_multi").removeClass("view"),$(".popup_multi").css({top:"",left:""})},setPopup:function(t){var i=$("body"),s="";function e(){!0!==popupModule.data.isMulti&&("function"==typeof t&&t(),$(".cpopup_box").removeClass("view"),$(".cpopup_box").css({top:"",left:""}),$(".bg_mask").remove(),i.css({overflow:""}),i.unbind("touchmove"))}s='<div class="bg_mask"></div>',i.append(s),i.css({overflow:"hidden"}),i.bind("touchmove",function(t){t.preventDefault()}),$(".layerClose").click(function(){e()}),i.on("click",function(t){var i=$(t.target),s=i.hasClass(".btnLayer"),o=i.hasClass("cpopup_box"),p=i.hasClass("bg_mask");$(".bg_mask"),s||o||!p||e()}),$(window).resize(function(){this.resizeTO&&clearTimeout(this.resizeTO),this.resizeTO=setTimeout(function(){$(this).trigger("resizeEnd")},0)}),$(window).on("resizeEnd",function(){popupModule.popCenter()})},popCenter:function(){var t=$(this.data.popupView),i=$(window).scrollLeft()+($(window).width()-t.width())/2,s=($(window).height()-t.height())/2;t.css({left:i,top:s})}};
