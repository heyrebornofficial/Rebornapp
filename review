<script type="text/javascript">
  
var loading = false;    //중복실행여부 확인 변수
var page = 1;   //불러올 페이지
var review_best = new Array(484,350,346,);

function view_thumb_slide(id){
	//console.log(111);
    eval("thumbslide_add(image_"+id+","+id+");");
    thumbslide();
}

/*nextpageload function*/
function next_load()
{
      // list up load

      $.ajax({
          url: "/review/more_review/",
          data: { 'page': page, 'cat':'0' },
          method: "POST",
          dataType: "json"
      })
      // HTTP
      .done(function(json) {
          // 이전
            var b_html = '';

            $.each(json.res, function(entryIndex, entry) {
                bp = entry.b_img;
                ap = entry.a_img;

                if(!entry.b_img){position_b=0;b_pic=Array();}else{position_b = bp.search(",");b_pic = bp.split(',');}
                if(!entry.a_img){position_a=0;a_pic=Array();}else{position_a = ap.search(",");a_pic = ap.split(',');}

                //if(position_b<0) bp = bp + ",";
                //if(position_a<0) ap = ap + ",";



            Array.isArray(b_pic);
            Array.isArray(a_pic);

            // 사진갯수
            var b_cnt = b_pic.length;if(b_pic[0]==""){b_cnt = b_cnt-1;}
            var a_cnt = a_pic.length;if(a_pic[0]==""){a_cnt = a_cnt-1;}
            var t_cnt = Number(b_cnt)+Number(a_cnt);

            var t_pic_cnt = b_cnt+a_cnt;

            var b_pic_cnt = b_cnt;
            var a_pic_cnt = a_cnt;

            if(t_pic_cnt > 6){
                var t_pic_cnt = 6;
                var t_cnt = 6;
                
                if(b_pic_cnt > 3){var b_pic_cnt = 3;var a_pic_cnt = 3;}
                if(b_pic_cnt == 1){var a_pic_cnt = 5;}
                if(b_pic_cnt == 2){var a_pic_cnt = 4;}
                if(b_pic_cnt == 3){var b_pic_cnt = 3;var a_pic_cnt = 3;}
            }

            // 사전후기 유저 프로필
            if(entry.eid == 0){
              var r_logo = "'/assets/images/reborn_notice_logo.png?v=2'";
            } else {
              var r_logo = "'/assets/images/reborn_notice_logo.png?v=2'";
            }

            // 티켓인지
            if(entry.mall_id > 0){
              var mall_img = '<img src="/assets/images/mall_ticket.png" width="45">';
            } else {
              var mall_img = '';
            }

            // 글쓴시간
            var r_date = entry.reg_date.substr(0, 10); //timeForToday(entry.reg_date);

     b_html += '<div class="review_item">';
     	b_html += '<div class="review_info">';
     		b_html += '<div class="thumb_area">';
     			b_html += '<span class="thumb" style="background-image:url('+r_logo+')"></span>';
     		b_html += '</div>';
     		b_html += '<div class="detail">';
     			b_html += '<span class="name">'+entry.user_nick+' '+mall_img
					if(inArray(entry.id,review_best)){
						b_html += '<span class="f_best">BEST</span>';
					}
     			b_html += '</span>';
     			b_html += '<span class="date">'+r_date+'</span>';
     		b_html += '</div>';
     	b_html += '</div>';
             
         	eval("image_"+entry.id+" = [];");	// 해당 이미지 목록
          	
  		b_html += '<div class="grid_photo item_'+t_cnt+'" id="grid_photo_'+entry.id+'" ';

         	if(t_pic_cnt<2){
         		b_html += ' style="margin-top:0px;" ';
         	}

        b_html += '>';

			if(t_pic_cnt >1){
  		
            for (var ii = 0; ii < b_pic_cnt; ii++) { // 배열 arr의 모든 요소의 인덱스(index)를 출력함.
            //document.write(i + " ");
			eval("image_"+entry.id+".push('"+b_pic[ii]+"');");
        	b_html += '<div class="item before" onclick="javascript:view_thumb_slide('+entry.id+');">';
        		b_html += '<span class="thumb_area">';
        			if(b_pic[ii].substr(-3) == "png" || b_pic[ii].substr(-3) == "PNG"){
            			b_html += '<img src="/img/review/'+b_pic[ii]+'" alt="" loading="lazy">';
        			}else{ 
                    	var tmpbimg = "/timg/thumb/400/0/?src="+encodeURI("/img/review/"+b_pic[ii]);
            			b_html += '<img src="'+tmpbimg+'" alt="" loading="lazy">';
        			}
        		b_html += '</span>';
        	b_html += '</div>';
             }

            for (var iii = 0; iii < a_pic_cnt; iii++) { // 배열 arr의 모든 요소의 인덱스(index)를 출력함.
            	eval("image_"+entry.id+".push('"+a_pic[iii]+"');");
            b_html += '<div class="item after" onclick="javascript:view_thumb_slide('+entry.id+');">';
            	b_html += '<span class="thumb_area">';
        			if(a_pic[iii].substr(-3) == "png" || a_pic[iii].substr(-3) == "PNG"){
            			b_html += '<img src="/img/review/'+a_pic[iii]+'" alt="" loading="lazy">';
        			}else{ 
                    	var tmpbimg = "/timg/thumb/400/0/?src="+encodeURI("/img/review/"+a_pic[iii]);
            			b_html += '<img src="'+tmpbimg+'" alt="" loading="lazy">';
        			}
            	b_html += '</span>';
            b_html += '</div>';
             }

			}else{
				console.log(entry.id + "---" + t_pic_cnt);
			}
        b_html += '</div>';
        b_html += '<div class="review_detail">';
        	b_html += '<div class="list_item">';
        		b_html += '<div class="detail">';
        			b_html += '<span class="sub">'+entry.cat1+' ('+entry.cat2+')</span>';
        			b_html += '<a href="/review/review_view/'+entry.id+'"><span class="tt">'+entry.title+'</span><span class="txt_more" >더보기</span></a>';
        		b_html += '</div>';
        	b_html += '</div>';
        	b_html += '<div class="list_info">';
        		b_html += '<div class="icon_info">';
        			b_html += '<span class="icon view">'+entry.hit+'</span>';
        			b_html += '<span class="icon comment">'+entry.reply+'</span>';
        			// b_html += '<span class="icon like">'+entry.good+'</span>';
        		b_html += '</div>';
        	b_html += '</div>';
        b_html += '</div>';
    b_html += '</div>';
             

            });

            $('.cont').append(b_html);


          page++; //페이지 증가
          loading = false;    //실행 가능 상태로 변경          
      })
      // HTTP
      .fail(function(xhr, status, errorThrown) {
          $("#text").html("Error!<br>")
          .append("err: " + errorThrown + "<br>")
          .append("stat: " + status);
      })
      // 
      .always(function(xhr, status) {
          $("#text").html("요청이 완료되었습니다!");
      });
}

$(window).scroll(function(){
    if($(window).scrollTop()+200>=$(document).height() - $(window).height())
    {
        if(!loading)    //실행 가능 상태라면?
        {
            loading = true; //실행 불가능 상태로 변경
            next_load(); 
        }
        else            //실행 불가능 상태라면?
        {
            //alert('다음페이지를 로딩중입니다.');  
        }
    }
});

function timeForToday(value) {
    const today = new Date();
    const timeValue = new Date(value);

    const betweenTime = Math.floor((today.getTime() - timeValue.getTime()) / 1000 / 60);

    if (betweenTime < 1) return '방금전';
    if (betweenTime < 60) {
        return `${betweenTime}분전`;
    }

    const betweenTimeHour = Math.floor(betweenTime / 60);
    if (betweenTimeHour < 24) {
        return `${betweenTimeHour}시간전`;
    }

    const betweenTimeDay = Math.floor(betweenTime / 60 / 24);
    if (betweenTimeDay < 365) {
        return `${betweenTimeDay}일전`;
    }

    return `${Math.floor(betweenTimeDay / 365)}년전`;
 }
</script>

