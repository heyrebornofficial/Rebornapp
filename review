        //alert(result.memo);
        /*
        if(type == "a"){
          num = parseInt(cnt)+1;
          $('#v_good_'+no).text(num);
        } else {
          num = parseInt(cnt)-1;
          $('#v_good_'+no).text(num);
        }
        */
      },
      error : function(result){
        alert('Error! - UBG');
      }
    });
  }  

