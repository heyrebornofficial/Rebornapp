<script type="text/javascript">
function inArray(needle,haystack){var length=haystack.length;for(var i=0;i<length;i++){if(haystack[i]==needle)return true;}return false;}

$(document).ready(function(){
	$("#go_mypage").on("click",function(){location.href="/member/mypage";});
	$("#go_notice").on("click",function(){location.href="/member/noti";});
	$("#go_search").on("click",function(){location.href="/search/index";});

	$("#c1search").on("change",function(){
		var order = $("#order_check").val();
		var afterstring = "";
		if(order){afterstring = "?order="+order;}
		if($("#c1search").val() == ""){
			location.href="/review"+afterstring;
		}else{
			location.href="/review/index/c1target/cat1/c1search/"+$("#c1search").val()+afterstring;
		}
	});

	$("#c2search").on("change",function(){
		var order = $("#order_check").val();
		var afterstring = "";
		if(order){afterstring = "?order="+order;}
		location.href="/review/index/c1target/cat1/c1search/"+$("#c1search").val()+"/c2target/cat2/c2search/"+$("#c2search").val()+afterstring;
	});

	$("#order_check").on("change",function(){
		var c1search = $("#c1search").val();
		var c2search = $("#c2search").val();
		
		var order = $("#order_check").val();
		location.href="/review/index/c1target/cat1/c1search/"+c1search+"/c2target/cat2/c2search/"+c2search+"?order="+order;
	});	
});
</script>
