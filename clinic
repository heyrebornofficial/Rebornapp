<script type="text/javascript">
$(document).ready(function(){
	$("#go_mypage").on("click",function(){location.href="/member/mypage";});
	$("#go_notice").on("click",function(){location.href="/member/noti";});
	$("#go_search").on("click",function(){location.href="/search/index";});

	// 의사페이지로 보내기
	$("#change_cli_doc").on("change",function(){location.href="/doctor/index";});

	$("#c1search").on("change",function(){

		var checked = $('#selectClinic').is(':checked');
		var skin_doctor = "y";
		if(!checked){skin_doctor = "n";}

	    search_val = "";
	    checked = $("input:checkbox[name='area']");
	    $.each(checked, function(index, item){
	     	if($(checked[index]).is(":checked")){
	        	console.log(index);
	        	search_val = $(checked[index]).val();
	    	};
	    });
	    //console.log(this.value);
	    //console.log(search_val);
	    c1search = this.value;
		location.href="/clinic/index/target/cli_subway/search/"+search_val+'/skin/'+skin_doctor+"?c1search="+c1search;
	});
});

function search_doctor(obj)
{
	var checked = $('#selectClinic').is(':checked');
	var skin_doctor = "y";
	if(!checked){skin_doctor = "n";}

	added_val = "";
	c1search = $("#c1search").val();
	if(c1search){added_val = "?c1search="+c1search;}
	
	location.href="/clinic/index/target/cli_subway/search/"+obj.value+'/skin/'+skin_doctor+added_val;
}

function search_skin(skin_val){
    checked = $("input:checkbox[name='area']");
    search_val = "";
    $.each(checked, function(index, item){
     	if($(checked[index]).is(":checked")){
        	console.log(index);
        	search_val = $(checked[index]).val();
    	};
    });

	added_val = "";
	c1search = $("#c1search").val();
	if(c1search){added_val = "?c1search="+c1search;}


	var checked2 = $('#selectClinic').is(':checked');
	
	location.href="/clinic/index/target/cli_subway/search/"+search_val+'/skin/'+skin_val+added_val;
}
</script>

